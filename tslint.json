{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "array-bracket-spacing": true,
    "linebreak-style": [
      true,
      "LF"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "strict-boolean-expressions": false,
    "variable-name": [
      true,
      "allow-pascal-case"
    ],
    "no-unused-variable": true
  },
  "rulesDirectory": []
}
